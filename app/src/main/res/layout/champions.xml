<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="0dp"
        android:layout_height="160dp"
        app:srcCompat="@mipmap/ic_launcher_round"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp" />

    <TextView
        android:id="@+id/championName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:text="TextView"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintLeft_toLeftOf="@+id/imageView" />

    <TextView
        android:id="@+id/championTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/championName" />

    <TextView
        android:id="@+id/description"
        android:layout_width="0dp"
        android:layout_height="80dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textMultiLine"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/championTitle" />

    <ImageView
        android:id="@+id/passiveIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/ic_launcher"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/description"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/description" />

    <ImageView
        android:id="@+id/qIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/ic_launcher"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/passiveIcon"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/description" />

    <ImageView
        android:id="@+id/wIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/ic_launcher"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/description"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/qIcon" />

    <ImageView
        android:id="@+id/eIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/ic_launcher"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/wIcon"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/description" />

    <ImageView
        android:id="@+id/rIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="@+id/description"
        app:layout_constraintTop_toBottomOf="@+id/eIcon"
        app:srcCompat="@mipmap/ic_launcher" />

    <TextView
        android:id="@+id/passiveText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintLeft_toRightOf="@+id/passiveIcon"
        android:layout_marginLeft="8dp"
        app:layout_constraintTop_toTopOf="@+id/passiveIcon"
        android:layout_marginTop="0dp" />

    <TextView
        android:id="@+id/qText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintLeft_toRightOf="@+id/qIcon"
        android:layout_marginLeft="8dp"
        app:layout_constraintTop_toTopOf="@+id/qIcon"
        android:layout_marginTop="0dp" />

    <TextView
        android:id="@+id/wText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintTop_toTopOf="@+id/wIcon"
        android:layout_marginTop="0dp"
        app:layout_constraintLeft_toRightOf="@+id/wIcon"
        android:layout_marginLeft="8dp" />

    <TextView
        android:id="@+id/eText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintLeft_toRightOf="@+id/eIcon"
        android:layout_marginLeft="8dp"
        app:layout_constraintTop_toTopOf="@+id/eIcon"
        android:layout_marginTop="0dp" />

    <TextView
        android:id="@+id/rText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/rIcon"
        app:layout_constraintTop_toTopOf="@+id/rIcon" />

</android.support.constraint.ConstraintLayout>